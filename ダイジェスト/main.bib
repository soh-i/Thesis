	Journal = {Nucleic Acids Res},
	Journal-Full = {Nucleic acids research},
	Mesh = {Genes; Genetic Predisposition to Disease; Genetic Variation; Genome, Human; Genomics; High-Throughput Screening Assays; Humans; Sequence Analysis, DNA; Software},
