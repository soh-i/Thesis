	Mesh = {Adenosine Deaminase; Animal Feed; Animals; Cell Line; Fats; Glucose; Insulin-Secreting Cells; Male; Mice; Obesity; RNA Editing; Rats; Receptors, Glutamate; Up-Regulation},
	Month = {Nov},
