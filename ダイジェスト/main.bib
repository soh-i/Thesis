@article{LaiDraNis9507,
	Abstract = {Mutagenic analysis of the substrate binding and catalytic domains of double-stranded RNA (dsRNA) adenosine deaminase (DRADA) was carried out. This nuclear enzyme is likely to be involved in the RNA editing of glutamate-gated ion channels that are essential for fast excitatory neurotransmission in mammalian brain. The deletion of the first or the third of the three dsRNA binding motifs within the substrate binding domain dramatically decreases enzyme activity, whereas the second motif seems to be dispensable. The results indicate that the three motifs are not functionally equivalent in the catalytic action of DRADA. Mutation of the putative zinc-coordinating residues, His910, Cys966, and Cys1036, abolished the DRADA activity. Similarly, the Glu912 residue, predicted to be involved in the proton transfer functions of the enzyme, was found to be indispensable. Our results reinforce the previous proposal that the hydrolytic deamination mechanism of DRADA may be more similar to that of the cytidine deaminases than of adenosine deaminases.},
	Author = {Lai, F and Drakas, R and Nishikura, K},
	Date-Added = {2014-01-14 06:16:04 +0000},
