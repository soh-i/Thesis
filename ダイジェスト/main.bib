	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {high-throughput sequencing; nucleo-cytoplasmic localization},
	Mesh = {Algorithms; Base Sequence; Cell Line; Computational Biology; Evolution, Molecular; High-Throughput Nucleotide Sequencing; Humans; MicroRNAs; Models, Genetic; Molecular Sequence Data; RNA Editing},
