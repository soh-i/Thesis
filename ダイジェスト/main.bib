	Journal = {J Virol},
	Journal-Full = {Journal of virology},
	Mesh = {Animals; Cell Line; Drosophila; Drosophila Proteins; Nodaviridae; Protein Binding; RNA Helicases; RNA Interference; RNA, Small Interfering; Ribonuclease III; Viral Proteins},
