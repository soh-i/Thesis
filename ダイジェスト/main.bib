	Mesh = {Adipose Tissue; Animals; Exons; Genome; High-Throughput Nucleotide Sequencing; Liver; Mice; Organ Specificity; Polymorphism, Single Nucleotide; RNA Editing; Sequence Analysis, DNA},
	Month = {Apr},
