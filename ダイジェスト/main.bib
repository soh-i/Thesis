	Mesh = {Amino Acid Sequence; Computational Biology; Exons; Genome, Human; Humans; Introns; Nucleotide Motifs; Open Reading Frames; Proteins; RNA Editing; Transcriptome},
	Month = {Sep},
	Number = {9},
	Pages = {1586-96},
	Pmc = {PMC3425774},
